{
  "files.associations": {
    "*.h": "c",
    "*.c": "c",
    "*.S": "asm",
    "*.inc": "c",
    "chrono": "c",
    "bitset": "c",
    "dynamic_bitset": "c",
    "cstdint": "c",
    "__bit_reference": "c",
    "__string": "c",
    "deque": "c",
    "string_view": "c",
    "future": "c",
    "string": "c",
    "rope": "c",
    "*.tcc": "c",
    "__locale": "c",
    "array": "c",
    "iterator": "c",
    "vector": "c",
    "typeindex": "c",
    "typeinfo": "c",
    "limits": "c",
    "type_traits": "c"
  },
  "editor.tabSize": 2,
  "editor.renderWhitespace": "all",
  "files.trimTrailingWhitespace": true,
  "editor.insertSpaces": true,
  "workbench.colorTheme": "Default Dark+",
  "editor.rulers": [
    80, 120
  ],
  "files.insertFinalNewline": true,
  "C_Cpp.loggingLevel": "Error"
}
